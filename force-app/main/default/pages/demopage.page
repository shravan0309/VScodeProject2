<apex:page standardController="contact" showHeader="false">
    <br/>
    
  <h1> Welcome to Canada </h1>
  
  <br/> <br/>
  
  My first name is <u>Shravan</u>
  
  <br/> <br/>
  
  My last name is <b><u>{!$User.LastName}</u></b>
  
  <br/> <br/>
  
  <apex:image url="data:image/jpeg;base64,"/>


<!-- Here we are going to create a page block. So to do that we have to create a tag using apex:form-->
<apex:form >

<apex:pageBlock title="Welcome to my page. Please Register Below"> <!-- Creates a page block with the title -->
<apex:pageBlockSection title="Enter details" columns="2"> <!-- Creates a section in the page block with the title. When the page block section is created, the field labels will be automatically visible -->

<apex:inputField value="{!contact.lastname}"/> <!-- apex:inputField tag creates a field where the customer can enter the details-->
<apex:inputField value="{!contact.firstname}"/>
<apex:inputField value="{!contact.email}"/>
<apex:inputField value="{!contact.phone}"/>



</apex:pageBlockSection>
</apex:pageBlock>

<!-- commandButton should be with in the form but out side the page block-->
<apex:commandButton value="Submit" action="{!Save}"/> <!-- Submit will be visible, if clicked the action takes place is save-->
<apex:commandButton value="Do not Submit" action="{!Cancel}"/> <!-- Do not Submit will be visible, if clicked the action takes place is cancel-->

</apex:form>

</apex:page>